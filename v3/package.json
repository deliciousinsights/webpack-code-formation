{
  "name": "webpack-first-steps-v3",
  "private": true,
  "version": "1.1.0",
  "description": "First Steps demo app for Webpack training, based on Webpack 3.x",
  "scripts": {
    "build": "webpack",
    "start": "webpack --watch"
  },
  "author": "Christophe Porteneuve <christophe@delicious-insights.com> (https://delicious-insights.com/)",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.3.3",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@gfx/zopfli": "^1.0.11",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "bundle-buddy": "^0.2.1",
    "clean-webpack-plugin": "^1.0.1",
    "compression-webpack-plugin": "=1.0.0",
    "copy-webpack-plugin": "^4.6.0",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.11",
    "eslint": "^5.14.1",
    "eslint-config-prettier": "^4.0.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^2.0.0",
    "hard-source-webpack-plugin": "^0.13.1",
    "html-webpack-inline-chunk-plugin": "^1.1.1",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^4.6.0",
    "node-sass": "^4.11.0",
    "nodemon": "^1.18.10",
    "postcss-cssnext": "^3.1.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.16.4",
    "regenerator-runtime": "^0.13.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "svg-url-loader": "^2.3.2",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "url-loader": "^1.1.2",
    "webpack": "^3.12.0",
    "webpack-assets-manifest": "^1.0.0",
    "webpack-bundle-analyzer": "^3.0.4",
    "webpack-dashboard": "^3.0.0",
    "webpack-dev-middleware": "^2.0.6",
    "webpack-dev-server": "^2.11.3",
    "webpack-hot-middleware": "^2.24.3",
    "webpack-merge": "^4.2.1",
    "webpack-monitor": "^1.0.14"
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "moment": "^2.24.0"
  },
  "eslintConfig": {
    "extends": [
      "standard",
      "prettier",
      "plugin:import/errors"
    ],
    "plugins": [
      "prettier",
      "import"
    ],
    "parser": "babel-eslint",
    "env": {
      "browser": true,
      "commonjs": true,
      "es6": true,
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "arrowParens": "always",
          "jsxSingleQuote": true,
          "semi": false,
          "singleQuote": true,
          "trailingComma": "es5"
        }
      ],
      "no-irregular-whitespace": 0
    }
  }
}
